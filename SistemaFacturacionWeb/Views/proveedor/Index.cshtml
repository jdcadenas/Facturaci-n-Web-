            @Html.DisplayFor(modelItem => item.RazonSocialProveedor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RifProveedor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TelefonoProveedor)
