            @Html.DisplayNameFor(model => model.RifProveedor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TelefonoProveedor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DireccionProveedor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DetallesProveedor)
        </th>
        <th>
