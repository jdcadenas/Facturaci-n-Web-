@model IEnumerable<SistemaFacturacionWeb.Models.proveedor>

@{
    ViewBag.Title = "Proveedor";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>
<table class="table">
    <tr>
